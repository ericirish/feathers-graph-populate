    "@feathersjs/errors": "^4.5.3",
    "@feathersjs/express": "^4.5.4",
    "@feathersjs/feathers": "^4.5.3",
    "ajv": "6.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "feathers-hooks-common": "^5.0.3",
    "feathers-nedb": "^5.1.0",
    "helmet": "^3.23.2",
    "lodash.merge": "^4.6.2",
